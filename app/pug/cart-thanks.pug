		// Контент страницы благодарности за покупку
		main.cart-thanks-content
			header.cart-thanks-header
				.container
					h2.cart-thanks__header-title Спасибо за покупку, Александр
					p.cart-thanks__header-details.common-indent В течение 15 минут наш менеджер свяжется для уточнения деталей заказа.
					a(href="#").cart-thanks-order-number.common-indent Мой заказ — 3456
			article.cart-thanks-profile-form
				.container
					h2.cart-thanks-form-title.common-indent Позвольте нам сделать для вас подарок
					p.cart-thanks-form-details.common-indent Укажите свой день рождения и гарантированно получите сюрприз
					form.cart-thanks-form(action method="POST")
						.cart-thanks-form-group.clearfix
							.sab-block.cart-thanks-sab-block
								.sex-block
									input(type="radio" name="sex" value="male" id="maleSex" checked="checked")
									label(for="maleSex")
									input(type="radio" name="sex" value="female" id="femaleSex")
									label(for="femaleSex")
								.birthday-block
									.birthday-block__heading Дата рождения:
									.select-date.cart-thanks-select-date 
										select.select-date__select(name="birth-day", id="birthDay")
											- for (var x = 1; x < 32; x++)
												option(value=x)=x
										select.select-date__select(name="birth-month",id="birthMonth")
											each val,index in ["января", "февраля", "марта", "апреля", "мая", "июня", "июля", "августа", "сентября", "октября", "ноября", "декабря"]
												option(value=index)= val
										select.select-date__select(name="birth-year", id="birthYear")
											-for (var x = 2016; x>1939; x--)
												option(value=x)= x
							.subscription-wrapper.clearfix
								p.subscription-title Укажите ваши увлечения
								.subscription.cart-thanks-subscription.clearfix
